#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa19ad20 .scope module, "clock_gen" "clock_gen" 2 17;
 .timescale 0 0;
v0xa177498_0 .var "clk", 0 0;
S_0xa177418 .scope module, "music" "music" 2 1;
 .timescale 0 0;
v0xa1973c0_0 .net "clk", 0 0, C4<z>; 0 drivers
v0xa197430_0 .var "counter", 15 0;
v0xa197490_0 .net "speaker", 0 0, L_0xa1974f0; 1 drivers
E_0xa197380 .event posedge, v0xa1973c0_0;
L_0xa1974f0 .part v0xa197430_0, 15, 1;
    .scope S_0xa19ad20;
T_0 ;
    %set/v v0xa177498_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0xa19ad20;
T_1 ;
    %delay 10, 0;
    %load/v 8, v0xa177498_0, 1;
    %inv 8, 1;
    %set/v v0xa177498_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0xa177418;
T_2 ;
    %vpi_call 2 4 "$dumpfile", "musicbox.vcd";
    %vpi_call 2 5 "$dumpvars", 1'sb0, v0xa1973c0_0, v0xa197490_0;
    %end;
    .thread T_2;
    .scope S_0xa177418;
T_3 ;
    %wait E_0xa197380;
    %load/v 8, v0xa197430_0, 16;
    %mov 24, 0, 16;
    %addi 8, 1, 32;
    %ix/load 0, 16, 0;
    %assign/v0 v0xa197430_0, 0, 8;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "musicbox.v";
